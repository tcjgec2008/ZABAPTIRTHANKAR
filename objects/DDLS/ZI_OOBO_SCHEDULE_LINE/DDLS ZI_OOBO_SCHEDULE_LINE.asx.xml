<SOURCE>@AbapCatalog.sqlViewName: 'ZI_OOBO_SCHEDLIN'
@AbapCatalog.compiler.compareFilter: true
@AccessControl.authorizationCheck: #CHECK
@EndUserText.label: 'OOBO Schedule line'
define view ZI_OOBOSCHEDULELINE as select from vbep {
    key vbeln as SalesDoc,
    key posnr as SalesItem,
   
@DefaultAggregation: #SUM
@Semantics.quantity.unitOfMeasure: 'confirmunit'
sum(bmeng)  as Confirm_Qty,
vrkme as confirmunit
    
}group by vbeln,posnr,vrkme
