<SOURCE>@AbapCatalog.sqlViewName: 'ZI_OOBO'
@AbapCatalog.compiler.compareFilter: true
@AccessControl.authorizationCheck: #CHECK
@EndUserText.label: 'Open Order Back Order'
define view ZI_OOBO_ORDER as select from vbap as OrderItem
left outer join zi_oobo_delivery as delivery
    on OrderItem.vbeln = delivery.referencedoc and OrderItem.posnr = delivery.refrenceitem join
    ZI_OOBOSCHEDULELINE as scheduleline on OrderItem.vbeln = scheduleline.SalesDoc and OrderItem.posnr = scheduleline.SalesItem{
    key OrderItem.vbeln as SalesDoc,
    key OrderItem.posnr as SalesItem,
    
    OrderItem.matnr as material,
    @DefaultAggregation: #SUM
    @Semantics.quantity.unitOfMeasure: 'OrderQuantityUnit'
    OrderItem.kwmeng as orderedQty,
    OrderItem.vrkme as OrderQuantityUnit,
    delivery.deliv_qty,
    scheduleline.Confirm_Qty,
 
    OrderItem.kwmeng  - deliv_qty as Open_Qty,
    OrderItem.kwmeng  - Confirm_Qty as BackOrder_Qty
    
    
    
    
    
}
