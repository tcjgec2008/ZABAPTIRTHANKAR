<SOURCE>@AbapCatalog.sqlViewName: 'ZI_DEL_BLKT1'
@AbapCatalog.compiler.compareFilter: true
@AccessControl.authorizationCheck: #CHECK
@EndUserText.label: 'Delivery Block Temporary View'
define view ZI_DEL_BLK_T1 as select from ZI_CDS_DELIV_AGG 
inner join ZI_CD_HDR_POS_DELV
    on ZI_CDS_DELIV_AGG.tabkey = ZI_CD_HDR_POS_DELV.tabkey and ZI_CDS_DELIV_AGG.date_time = ZI_CD_HDR_POS_DELV.Date_time_temp 
  {
  ZI_CDS_DELIV_AGG.Salesorder as SO,
  ZI_CDS_DELIV_AGG.SalesItem as SO_ITEM,
  ZI_CDS_DELIV_AGG.Schedline as SO_LINE,
  ZI_CD_HDR_POS_DELV.blkreleasedate as blkreldate,
  '' as Deliv_blk
}
where value_new = ''
union
select from vbep 
{ 
vbeln as SO, posnr as SO_ITEM,etenr as SO_LINE, '' as blkreldate,  lifsp as DELIV_BLK
} where lifsp &lt;&gt; ''
