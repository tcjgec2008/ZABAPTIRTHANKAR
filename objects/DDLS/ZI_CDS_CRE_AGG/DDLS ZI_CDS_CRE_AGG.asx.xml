<SOURCE>@AbapCatalog.sqlViewName: 'ZCREDITBLOCKAGG'
@AbapCatalog.compiler.compareFilter: true
@AccessControl.authorizationCheck: #CHECK
@EndUserText.label: 'Credit block Aggregation'
define view ZI_CDS_CRE_AGG as select from ZI_CDHDR as _cdhdr
{
   key _cdhdr.objectid,
    
   max(_cdhdr.Date_time) as MAX_DATE
    
} where((_cdhdr.objectclas = 'VERKBELEG') and (_cdhdr.udate &gt; '20170101')) 
group by _cdhdr.objectid
    
