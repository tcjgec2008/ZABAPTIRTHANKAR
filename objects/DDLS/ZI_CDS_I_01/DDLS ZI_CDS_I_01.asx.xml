<SOURCE>@AbapCatalog.sqlViewName: 'ZCDPOS'
@AbapCatalog.compiler.compareFilter: true
@AccessControl.authorizationCheck: #CHECK
@EndUserText.label: 'CDPOS'
define view ZI_CDS_I_01 as select from cdhdr
join cdpos as _cdpos on cdhdr.objectid = _cdpos.objectid
{
    key cdhdr.objectid,
    key cdhdr.objectclas,
    key cdhdr.changenr,
    cdhdr.udate,
    cdhdr.utime,
    _cdpos.tabkey,
    _cdpos.value_new,
    _cdpos.value_old,
    substring(_cdpos.tabkey, 1,10) as Sales_order,
    substring(_cdpos.tabkey, 14,6) as Sales_doc_item,
    substring(_cdpos.tabkey, 20,4) as Sched_line,
    concat(cdhdr.udate, cdhdr.utime) as Date_time
    
}

